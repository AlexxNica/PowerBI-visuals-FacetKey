{
  "name": "essex.widget.facetkey",
  "version": "0.2.2",
  "description": "PowerBI custom visual.",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "preinstall:prereq": "npm install tar.gz mv",
    "preinstall:submodules": "node bin/downloadPrivateSubmodules",
    "preinstall:dependencies": "node bin/installSubmoduleDeps",
    "preinstall": "npm run clean && npm run preinstall:prereq && npm run preinstall:submodules && npm run preinstall:dependencies",
    "install-certificate": "node bin/openCert",
    "start": "npm-run-all --parallel dev watch:templates",
    "tdd": "npm-run-all --parallel dev watch:templates test:tdd",
    "dev": "node bin/startDev",
    "watch:templates": "onchange 'lib/uncharted-facets/public/templates/**/*' -- npm run compiletemplates",
    "compiletemplates": "node bin/compileHandlebars",
    "package": "mkdirp dist && npm run compiletemplates && npm run package:code && node bin/packageVisual $npm_package_config_tmpProd",
    "package:code": "mkdirp $npm_package_config_tmpProd && npm-run-all --parallel package:script package:style",
    "package:script": "webpack --color --output-filename $npm_package_config_tmpProd/visual.js | sed '/node_modules/ { N; N; d; }'",
    "package:style": "node-sass $(json -f pbiviz.json style) > $npm_package_config_tmpProd/visual.css",
    "test": "karma start --color | sed '/node_modules/ { N; N; d; }'",
    "test:tdd": "karma start --color --tdd | sed '/node_modules/ { N; N; d; }'",
    "clean": "rm -rf node_modules .tmp"
  },
  "config": {
    "tmpDrop": "./.tmp/drop",
    "tmpProd": "./.tmp/prod"
  },
  "keywords": [],
  "author": "Uncharted Software Inc.",
  "license": "MIT",
  "privateSubmodules": {
    "uncharted-facets": "2.3.0"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.33",
    "@types/lodash": "^4.14.37",
    "@types/mocha": "^2.2.32",
    "@types/requirejs": "^2.1.28",
    "@types/sinon": "^1.16.31",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "chokidar": "^1.6.1",
    "connect": "^3.5.0",
    "handlebars": "^4.0.5",
    "inject-loader": "^3.0.0-beta2",
    "json": "^9.0.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.0",
    "node-zip": "^1.1.1",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "powerbi-visuals": "^0.5.9",
    "serve-static": "^1.11.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.16.3"
  }
}
